{
  "name": "react-flow-z",
  "version": "1.0.0-z",
  "description": "A lightweight async flow runtime for orchestrating side effects with explicit control over cancellation, debounce, throttling, and execution order.",
  "license": "MIT",
  "author": "Delpi.Kye",
  "sideEffects": false,
  "type": "module",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.esm.js",
      "require": "./build/index.cjs.js"
    }
  },
  "files": ["build"],
  "scripts": {
    "clean": "rimraf build",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "cb": "npm run clean && npm run build",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run cb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delpikye-v/react-flow.git"
  },
  "homepage": "https://github.com/delpikye-v/react-flow#readme",
  "bugs": {
    "url": "https://github.com/delpikye-v/react-flow/issues"
  },
  "keywords": [
    "async-flow",
    "async-runtime",
    "side-effects",
    "effect-orchestration",
    "cancellation",
    "debounce",
    "throttle",
    "takeLatest",
    "queue",
    "abort-controller",
    "flow-control",
    "headless",
    "framework-agnostic",
    "typescript"
  ],
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.0",

    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
